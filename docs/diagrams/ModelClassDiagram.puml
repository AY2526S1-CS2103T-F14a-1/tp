@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyFoodBook" as ReadOnlyFoodBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class FoodBook
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class Person
Class Address
Class Email
Class Name
Class Phone
Class Tag

Class UniqueDeliveryList
Class Delivery
Class Datetime
Class String
Class Integer

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

FoodBook .up.|> ReadOnlyFoodBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyFoodBook
ModelManager -left-> "1" FoodBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

FoodBook *--> "1" UniquePersonList
UniquePersonList --> "~* all" Person
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> "*" Tag

FoodBook *--> "1" UniqueDeliveryList
UniqueDeliveryList --> "~* all" Delivery
Delivery *--> Integer
Delivery *--> "Client" Person
Delivery *--> Datetime
Delivery *--> "Remarks" String
Delivery *--> "Tag" String

Person -[hidden]up--> I
UniquePersonList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Person
ModelManager --> "~* filtered" Delivery
@enduml
