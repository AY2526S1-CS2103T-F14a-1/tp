@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":FoodBookParser" as FoodBookParser LOGIC_COLOR
participant ":DeleteClientCommandParser" as DeleteClientCommandParser LOGIC_COLOR
participant "d:DeleteClientCommand" as DeleteClientCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("delete_client Alex Yeoh")
activate LogicManager

LogicManager -> FoodBookParser : parseCommand("delete_client Alex Yeoh")
activate FoodBookParser

create DeleteClientCommandParser
FoodBookParser -> DeleteClientCommandParser
activate DeleteClientCommandParser

DeleteClientCommandParser --> FoodBookParser
deactivate DeleteClientCommandParser

FoodBookParser -> DeleteClientCommandParser : parse("Alex Yeoh")
activate DeleteClientCommandParser

create DeleteClientCommand
DeleteClientCommandParser -> DeleteClientCommand
activate DeleteClientCommand

DeleteClientCommand --> DeleteClientCommandParser :
deactivate DeleteClientCommand

DeleteClientCommandParser --> FoodBookParser : d
deactivate DeleteClientCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteClientCommandParser -[hidden]-> FoodBookParser
destroy DeleteClientCommandParser

FoodBookParser --> LogicManager : d
deactivate FoodBookParser

LogicManager -> DeleteClientCommand : execute(m)
activate DeleteClientCommand

DeleteClientCommand -> Model : deletePerson(Alex Yeoh)
activate Model

Model --> DeleteClientCommand
deactivate Model

DeleteClientCommand -> Model : deleteDelivery(associated deliveries)
activate Model

Model --> DeleteClientCommand
deactivate Model

create CommandResult
DeleteClientCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteClientCommand
deactivate CommandResult

DeleteClientCommand --> LogicManager : r
deactivate DeleteClientCommand

[<--LogicManager
deactivate LogicManager
@enduml
